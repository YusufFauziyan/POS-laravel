{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "lib": ["ESNext", "DOM"],
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["./resources/js/*"]
    },
    "resolveJsonModule": true,
    "types": ["vite/client"],
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["resources/js/**/*.ts", "resources/js/**/*.d.ts", "resources/js/**/*.vue"],
  "exclude": ["node_modules"]
}
